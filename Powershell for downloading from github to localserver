# URL du fichier spécifique à télécharger (remplacez par l'URL de votre fichier)
$fileUrl = "https://github.com/Kartes37/Populate-an-active-Directory-by-script.git"

# Chemin local où le fichier sera enregistré
$destinationPath = "C:\Users\Administrateur\Documents\TestScriptAD"

# Télécharger le fichier
Invoke-WebRequest -Uri $fileUrl -OutFile $destinationPath

Write-Host "Le fichier a été téléchargé avec succès."
